{
  "name": "branches",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/branches",
  "projectType": "application",
  "projectId": "prj_NqBSR0oCTmQTZlsi3Oq5qn8bOWfT",
  "orgId": "team_3FcuJksDBvOKgDoo28ETlJjx",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/branches"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/branches"
        },
        "production": {}
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx serve api",
          "nx serve branches",
          "open http://localhost:4200/?domain=tacomagooners.com"
        ],
        "parallel": true
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "branches:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "branches:build:development",

          "dev": true
        },
        "production": {
          "buildTarget": "branches:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "branches:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/branches/jest.config.ts"
      }
    },
    "stylelint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx stylelint apps/branches/**/*.{css,scss}",
        "fix": true,
        "maxWarnings": 0
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "fix": true,
        "maxWarnings": 0
      }
    }
  }
}
