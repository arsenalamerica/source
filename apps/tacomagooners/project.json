{
  "name": "tacomagooners",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tacomagooners",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/tacomagooners"
      },
      "configurations": {
        "production": {},
        "development": {
          "outputPath": "apps/tacomagooners"
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "tacomagooners:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "tacomagooners:build:production",
          "dev": false
        },
        "development": {
          "buildTarget": "tacomagooners:build:development",
          "dev": true
        }
      },
      "defaultConfiguration": "development"
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "tacomagooners:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/tacomagooners"],
      "options": {
        "jestConfig": "apps/tacomagooners/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "stylelint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx stylelint apps/tacomagooners/**/*.{css,scss}"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tacomagooners/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
