{
  "name": "vancouverarsenal",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/vancouverarsenal",
  "projectType": "application",
  "targets": {
    "icons": {
      "executor": "@arsenalamerica/nx-next-icons:favicons",
      "options": {
        "svgLogoPath": "{projectRoot}/assets/logo.svg",
        "iconOutputDir": "{projectRoot}/public"
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/vancouverarsenal"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/vancouverarsenal"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vancouverarsenal:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "vancouverarsenal:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "vancouverarsenal:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "vancouverarsenal:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/vancouverarsenal/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "stylelint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx stylelint apps/vancouverarsenal/**/*.{css,scss}"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/vancouverarsenal/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
