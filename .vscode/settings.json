{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "same"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.validate": false,
  "cssModules.camelCase": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "emmet.excludeLanguages": ["markdown", "scss"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "html": {
      "attr_quotes": "single"
    },
    "javascript": {
      "self_closing_tag": true
    }
  },
  "emmet.triggerExpansionOnTab": true,
  "eslint.enable": true,
  "eslint.validate": [
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.associations": {
    "CODEOWNERS*": "plaintext"
  },
  "less.validate": false,
  "prettier.requireConfig": true,
  "scss.validate": false,
  "somesass.suggestFromUseOnly": true,
  "stylelint.validate": ["css", "scss"],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#a01a24",
    "activityBar.activeBorder": "#ffffff",
    "activityBar.background": "#da1f26",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#a01a24",
    "activityBarBadge.background": "#ffffff",
    "activityBarBadge.foreground": "#da1f26",
    "list.activeSelectionBackground": "#da1f26",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#a01a24",
    "list.hoverForeground": "#fff",
    "sideBar.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#c4222e",
    "sideBarSectionHeader.foreground": "#ffffff"
  },
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": true,
  "coverage-gutters.customizable.status-bar-toggler-watchCoverageAndVisibleEditors-enabled": true,
  "coverage-gutters.customizable.context-menu": true
}
