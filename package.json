{
  "name": "tacomagooners",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,sass,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@bjeco/prettier-config",
  "eslintConfig": {
    "extends": [
      "@bjeco",
      "next",
      "next/core-web-vitals"
    ]
  },
  "stylelint": {
    "extends": "@bjeco/stylelint-config"
  },
  "devDependencies": {
    "@bjeco/eslint-config": "latest",
    "@bjeco/prettier-config": "latest",
    "@bjeco/stylelint-config": "latest",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "36.0.0",
    "husky": "1.3.1",
    "lint-staged": "11.1.2"
  },
  "packageManager": "yarn@3.0.2"
}
