{
  "name": "@arsenalamerica/source",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "api": "yarn nx serve api --port=3333",
    "build": "yarn nx run-many --all --target=build --parallel=3",
    "test": "yarn nx run-many --all --target=test --parallel=3",
    "lint": "yarn nx run-many --all --target=lint --parallel=3",
    "e2e": "yarn nx run-many --all --target=e2e",
    "start": "yarn nx start branches",
    "nx-latest": "yarn nx migrate latest",
    "nx-migrate": "yarn install && nx migrate --run-migrations",
    "postinstall": "husky install"
  },
  "engines": {
    "node": "18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
