  "private": true,
  "scripts": {
    "api": "yarn nx serve api --port=3333",
    "all": "yarn nx run-many --targets=lint,test,build --skip-nx-cache",
    "build": "yarn nx run-many --all --target=build",
    "test": "yarn nx run-many --all --target=test",
    "lint": "yarn nx run-many --all --target=lint",
    "e2e": "yarn nx run-many --all --target=e2e",
    "start": "yarn nx start branches",
    "nx-latest": "NX_MIGRATE_USE_LOCAL=true yarn nx migrate latest",
    "nx-migrate": "NX_MIGRATE_USE_LOCAL=true nx migrate --run-migrations",
    "postinstall": "husky"
  },
  "engines": {
    "node": "20"
  },
